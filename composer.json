{
    "name": "sonata-project/article-bundle",
    "type": "symfony-bundle",
    "description": "Advanced article management",
    "license": "MIT",
    "authors": [
        {
            "name": "Benoit Maziere",
            "email": "benoit.maziere@gmail.com"
        },
        {
            "name": "Sonata Community",
            "homepage": "https://github.com/sonata-project/SonataArticleBundle/contributors"
        }
    ],
    "require": {
        "php": "^7.1",
        "sonata-project/admin-bundle": "^3.1",
        "sonata-project/classification-bundle": "^3.0",
        "sonata-project/doctrine-orm-admin-bundle": "^3.0",
        "sonata-project/easy-extends-bundle": "^2.2",
        "sonata-project/media-bundle": "^3.0",
        "symfony/config": "^3.4 || ^4.0",
        "symfony/dependency-injection": "^3.4 || ^4.0",
        "symfony/form": "^3.4 || ^4.0",
        "symfony/http-foundation": "^3.4 || ^4.0",
        "symfony/options-resolver": "^3.4 || ^4.0",
        "symfony/routing": "^3.4 || ^4.0",
        "symfony/security-core": "^3.4 || ^4.0",
        "symfony/templating": "^3.4 || ^4.0",
        "symfony/translation": "^3.4 || ^4.0",
        "symfony/twig-bridge": "^3.4 || ^4.0",
        "symfony/validator": "^3.4 || ^4.0",
        "twig/extensions": "^1.0",
        "twig/twig": "^2.0"
    },
    "conflict": {
        "sonata-project/block-bundle": "<3.11"
    },
    "require-dev": {
        "matthiasnoback/symfony-config-test": "^4.0",
        "matthiasnoback/symfony-dependency-injection-test": "^4.0",
        "sonata-project/block-bundle": "^3.18.3",
        "symfony/phpunit-bridge": "^5.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Sonata\\ArticleBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sonata\\ArticleBundle\\Tests\\": "tests/"
        }
    }
}
